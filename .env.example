# Example environment config for Minecraft server JAVA Edition
# Copy this file to .env and fill in the values

# EULA must be set to TRUE to run the server
EULA=TRUE

# Minecraft server version
VERSION=1.21.8

# SHA1 hash of the server.jar file
SHA1=6bce4ef400e4efaa63a13d5e6f6b500be969ef81

# Maximum number of players allowed on the server
MAX_PLAYERS=10

# Message of the Day (MOTD) for the server
MOTD=Welcome to Minecraft!

# Memory allocation for the server
MEMORY=1024M

# RCON settings
RCON_PASSWORD=SuperSecret